{"format_version":"1.10.0","animation_controllers":{"controller.animation.frog.base":{"initial_state":"on_land","states":{"on_land":{"animations":[{"walk":"math.min(1.5,query.modified_move_speed*10)"},"idle"],"blend_transition":0.1,"transitions":[{"in_water":"query.is_in_water"},{"in_air":"!query.is_on_ground"}]},"in_water":{"animations":["controller_swim"],"blend_transition":0.1,"transitions":[{"on_land":"!query.is_in_water&&q.is_on_ground"}]},"in_air":{"blend_transition":0.1,"transitions":[{"in_water":"query.is_in_water"},{"on_land":"q.is_on_ground"}]}}},"controller.animation.frog.particles":{"states":{"default":{"transitions":[{"night":"q.time_of_day<0.25||q.time_of_day>0.78"}]},"night":{"particle_effects":[{"effect":"mysterious_glowy_bug"}],"transitions":[{"default":"q.time_of_day>0.25&&q.time_of_day<0.78"}]}}},"controller.animation.frog.swim":{"states":{"default":{"animations":["idle_water"],"blend_transition":0.16,"transitions":[{"moving":"query.modified_move_speed>0.15"}]},"moving":{"animations":[{"swim":"math.min(1.0,query.modified_move_speed*10)"}],"blend_transition":0.16,"transitions":[{"default":"query.modified_move_speed<0.1"}]}}},"controller.animation.frog.blink":{"initial_state":"a","states":{"a":{"animations":["blink_a"],"transitions":[{"b":"q.any_animation_finished"}]},"b":{"animations":["blink_b"],"transitions":[{"a":"q.any_animation_finished"}]}}},"controller.animation.frog.eat_mob":{"initial_state":"default","states":{"default":{"transitions":[{"eating":"query.is_eating_mob"}]},"eating":{"animations":["tongue"],"transitions":[{"default":"query.all_animations_finished"}]}}}}}