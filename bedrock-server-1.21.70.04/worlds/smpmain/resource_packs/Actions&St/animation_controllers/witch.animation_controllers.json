{"format_version":"1.10.0","animation_controllers":{"controller.animation.witch.base":{"states":{"default":{"transitions":[{"potion":"q.get_equipped_item_name=='potion'"},{"milk":"q.get_equipped_item_name=='milk_bucket'"}]},"potion":{"animations":["drink"],"particle_effects":[{"effect":"potion_0"},{"effect":"potion_1"}],"blend_transition":0.24,"transitions":[{"default":"q.all_animations_finished||q.get_equipped_item_name!='potion'"}]},"milk":{"animations":["drink"],"blend_transition":0.24,"transitions":[{"default":"q.all_animations_finished||q.get_equipped_item_name!='milk_bucket'"}]}}},"controller.animation.witch.target":{"states":{"default":{"animations":["target","base_pose","blink"],"blend_transition":0.15,"transitions":[{"drinking":"q.get_equipped_item_name=='potion'||q.get_equipped_item_name=='milk_bucket'"},{"is_scared":"q.is_avoiding_mobs"}]},"drinking":{"animations":["target_potion","base_pose","blink","arms_lock"],"blend_transition":0.2,"transitions":[{"default":"q.get_equipped_item_name!='potion'&&q.get_equipped_item_name!='milk_bucket'"}]},"is_scared":{"animations":["target_potion","base_pose","scared","contraction"],"blend_transition":0.2,"transitions":[{"default":"!q.is_avoiding_mobs"}]}}},"controller.animation.witch.move":{"states":{"default":{"animations":[{"move":"q.modified_move_speed*1.3"}],"blend_transition":0.16,"transitions":[{"float":"q.is_in_water"},{"in_air":"!q.is_on_ground"}]},"float":{"animations":["float"],"blend_transition":0.2,"transitions":[{"default":"!q.is_in_water&&q.is_on_ground"}]},"in_air":{"blend_transition":0.16,"transitions":[{"float":"q.is_in_water"},{"default":"q.is_on_ground"}]}}}}}