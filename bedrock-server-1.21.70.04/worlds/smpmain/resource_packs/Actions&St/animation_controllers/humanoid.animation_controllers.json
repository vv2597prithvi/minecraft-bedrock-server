{"format_version":"1.10.0","animation_controllers":{"controller.animation.humanoid.base_pose":{"initial_state":"default","states":{"default":{"animations":["humanoid_base_pose"]}}},"controller.animation.humanoid.target_anim":{"states":{"default":{"animations":["look_at_target_default"],"blend_transition":0.15,"transitions":[{"horizontal":"q.is_gliding"}]},"horizontal":{"animations":["look_at_target_gliding",{"look_at_target_inverted":"1/math.max(1,v.gliding_speed_value*0.6)"}],"blend_transition":0.15,"transitions":[{"default":"!q.is_gliding"}]}}},"controller.animation.humanoid.riding":{"initial_state":"default","states":{"default":{"transitions":[{"riding":"query.is_riding"}]},"riding":{"animations":["riding.arms","riding.legs"],"transitions":[{"default":"!query.is_riding"}]}}},"controller.animation.player_anim.swim":{"states":{"default":{"animations":[{"crawling":"math.cos(variable.swim_amount*180)*-0.5+0.5"}],"blend_transition":0.2,"transitions":[{"swim":"!q.is_on_ground&&q.vertical_speed==0"},{"dive":"!q.is_on_ground"}]},"swim":{"particle_effects":[{"effect":"swim_ripples"}],"animations":[{"swim_rot":"math.cos(variable.swim_amount*180)*-0.5+0.5"},{"swimming":"variable.swim_amount"}],"blend_transition":0.2,"transitions":[{"default":"q.is_on_ground"},{"dive":"q.vertical_speed!=0"}]},"dive":{"animations":[{"crawling.legs":"math.cos(variable.swim_amount*180)*-0.5+0.5"}],"blend_transition":0.3,"transitions":[{"default":"q.is_on_ground"},{"swim":"q.vertical_speed==0"}]}}}}}