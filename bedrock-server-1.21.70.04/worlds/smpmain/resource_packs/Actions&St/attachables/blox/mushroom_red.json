{
  "format_version":"1.10.0",
  "minecraft:attachable":{
    "description":{
      "identifier":"minecraft:red_mushroom",
      "materials":{
	"default":"entity"
      },
      "textures":{
	"default":"textures/models/held_item/mushrooms/fly",
	"prop":"textures/models/held_item/mushrooms/big_red"
      },
      "geometry":{
	"default":"geometry.mush_ow",
	"prop":"geometry.fly_bed"
      },
      "render_controllers":[{"controller.render.default":"!v.is_prop"},{"controller.render.prop":"v.is_prop"}],
      "animations":{
	"3rd":"animation.item.mush",
	"1st":"animation.item.first_person_mush",
	"wiggle":"animation.mushroom.wiggle",
	"controller_wiggle":"controller.animation.prop.wiggle",
	"controller_prop":"controller.animation.prop"
      },
      "scripts":{
	"initialize":["variable.is_prop = 0;"],
	"animate":[
	  "controller_prop",
	  {"controller_wiggle":"variable.is_prop == 1"}
	]
      }
    }
  }
}
