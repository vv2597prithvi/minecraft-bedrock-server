{
  "format_version":"1.8.0",
  "render_controllers":{
    "controller.render.light":{
      "geometry":"Geometry.default",
      "materials":[
	{"*":"Material.default"},
	{"flame_y":"Material.flare"}
      ],
      "textures":["q.is_breathing&&!q.is_swimming ? Texture.on:Texture.off"]
    },
    "controller.render.lantern":{
      "arrays":{
	"textures":{"Array.skins":["Texture.default","Texture.off"]}
      },
      "geometry":"geometry.default",
      "materials":[{"*":"Material.default"}],
      "textures":["Array.skins[q.is_in_water]"]
      }
    }
  }
}
